{
  "name": "adahide",
  "version": "0.0.1",
  "engines": {
    "node": "16"
  },
  "description": "A simple Cardano wallet",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vacuumlabs/adalite.git"
  },
  "keywords": [
    "cardano",
    "wallet"
  ],
  "author": "Vacuumlabs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vacuumlabs/adalite/issues"
  },
  "homepage": "https://github.com/vacuumlabs/adalite#readme",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config webpack.build.config.js",
    "install": "cd app && yarn install && cd ..",
    "dev": "yarn generate-cert",
    "test": "TS_NODE_PROJECT='./tsconfig.test.json' mocha",
    "eslint": "eslint . --max-warnings=0 --ext .ts,.tsx,.js,.jsx",
    "fix": "prettier-eslint --write \"{{src,tests/src}/**/*.{ts,tsx,js,jsx,json,css},package.json}\"",
    "generate-cert": "test -e server.cert && echo \"certificate already exists\" || openssl req -nodes -new -x509 -keyout server.key -out server.cert -subj \"/C=US\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{{src,tests/src}/**/*.{ts,tsx,js,jsx,json,css},package.json}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "directories": {
    "src": "./src",
    "test": "./tests"
  },
  "dependencies": {
    "@cardano-ogmios/client": "^5.5.7",
    "@sentry/node": "^6.15.0",
    "babel-regenerator-runtime": "^6.5.0",
    "body-parser": "^1.18.2",
    "check-types": "^8.0.2",
    "compression": "^1.7.2",
    "device": "^0.3.9",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-basic-auth": "^1.2.0",
    "express-ipfilter": "^1.1.2",
    "express-mung": "^0.5.1",
    "glob": "^7.2.0",
    "isomorphic-fetch": "^3.0.0",
    "normalize-url": "^4.0.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "universal-analytics": "^0.5.3",
    "ws": "^8.4.2",
    "@cardano-foundation/ledgerjs-hw-app-cardano": "^5.1.0",
    "@ledgerhq/hw-transport": "^5.51.1",
    "@ledgerhq/hw-transport-u2f": "^5.36.0-deprecated",
    "@ledgerhq/hw-transport-webhid": "^5.51.1",
    "@ledgerhq/hw-transport-webusb": "^5.53.1",
    "@sentry/browser": "^6.15.0",
    "@trezor/connect-web": "9.0.0",
