    "mini-css-extract-plugin": "^1.5.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^7.1.0",
    "release-it": "^15.4.2",
    "sass": "^1.32.11",
