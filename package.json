{
  "name": "adahide",
  "version": "0.0.1",
  "engines": {
    "node": "18"
  },
  "description": "A simple Cardano wallet",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoeldevapps/adahide.git"
  },
  "keywords": [
    "cardano",
    "wallet"
  ],
  "author": "Zoeldev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zoeldevapps/adahide/issues"
  },
  "homepage": "https://github.com/zoeldevapps/adahide#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "mocha --require=./tests/babel-register.js",
    "eslint": "eslint . --max-warnings=0 --ext .ts,.tsx,.js,.jsx",
    "fix": "prettier-eslint --write \"{{src,tests/src}/**/*.{ts,tsx,js,jsx,json,css},package.json}\"",
    "postinstall": "patch-package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{{src,tests/src}/**/*.{ts,tsx,js,jsx,json,css},package.json}": [
      "prettier-eslint --write"
    ]
  },
  "directories": {
    "src": "./src",
    "test": "./tests"
  },
  "dependencies": {
    "@cardano-foundation/ledgerjs-hw-app-cardano": "^5.1.0",
    "@cardano-ogmios/client": "^5.5.7",
    "@ledgerhq/hw-transport": "^6.27.6",
    "@ledgerhq/hw-transport-u2f": "^5.36.0-deprecated",
    "@ledgerhq/hw-transport-webhid": "^6.27.6",
    "@ledgerhq/hw-transport-webusb": "^6.27.6",
    "@sentry/react": "^7.17.3",
    "@sentry/tracing": "^7.17.3",
    "@trezor/connect-web": "^9.0.3",
    "assert": "^2.0.0",
    "bignumber.js": "^9.1.0",
    "bip39-light": "^1.0.7",
    "bitbox02-api": "^0.15.0",
    "buffer": "^6.0.3",
    "cardano-crypto.js": "^6.1.1",
    "cbor": "^8.1.0",
    "chacha": "^2.1.0",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "path-browserify": "^1.0.1",
    "pbkdf2": "^3.1.2",
    "process": "^0.11.10",
    "qrious": "^4.0.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "unistore": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@rollup/plugin-inject": "^5.0.2",
    "@types/fetch-mock": "^6",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-vacuumlabs": "^8.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fetch-mock": "^6",
    "husky": "^4.3.5",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "mocha": "^10.1.0",
    "patch-package": "^6.5.0",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^7.1.0",
    "release-it": "^15.4.2",
    "sass": "^1.55.00",
    "typescript": "^4.8.4",
    "vite": "^3.2.1"
  }
}
