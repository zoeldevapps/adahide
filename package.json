    "bitbox02-api": "^0.13.0",
    "cardano-crypto.js": "^6.1.0",
    "cbor": "^8.1.0",
    "chacha": "^2.1.0",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pbkdf2": "^3.1.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "unistore": "^3.5.2"
