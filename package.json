    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-eslint": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.3.6",
    "css-loader": "^5.2.2",
    "eslint": "^8.25.0",
